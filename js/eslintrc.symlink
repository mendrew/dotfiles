{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    ".project-eslintrc"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 0,
    "semi": 2
  },
  "indent": 2
}
